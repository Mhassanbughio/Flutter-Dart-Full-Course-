##################### Arithmetic = , != , >,  < ,>=, <= 


main(){
  
  var a=15;
  var b=10;
  print(a+b);
  print(a-b);
  print(a*b);
  print(a/b);
  print(a%b);
  print(a~/b);
  
}

###########3##### Relational  = , != , >,  < ,>=, <= 


main(){
  
  var a=15;
  var b=10;
  
  print(a!=b);
  print(a>b);
  print(a<b);
  print(a<=b);
  print(a>=b);
  print(a==b);
  
}
