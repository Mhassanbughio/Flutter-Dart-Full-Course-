main(){
  int a= 15;
  int b=30;
  
  if(a==10 && b==30){
    print("equal");
  }
    else{
      print("not eqaul"); ///output
}
}

main(){
  int a=15;
  int b=30;
  
  if(a==10 && b==30){
    print("equal");
  }
    else{
      print("not eqaul"); // not eqaul
      }
}

main(){
 
  int x=20;
  int y=30;
  
  print(!(x<y));
  
if(x==10 || y==20)
{
  print("equal"); // ouput will be equal
}
  else{
    print("Not equal");
  }
}
